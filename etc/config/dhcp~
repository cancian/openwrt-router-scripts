
config dnsmasq
	option local '/lan/'
	option leasefile '/tmp/dhcp.leases'
	option filterwin2k '1'
	option nonegcache '1'
	option sequential_ip '1'
	list dns '192.168.1.1'
	list dns '8.8.8.8'
	list dns '8.8.4.4'
	list dns '1.1.1.1'
	list dns '1.0.0.1'
	list dns '2001:4860:4860::8888'
	list dns '2001:4860:4860::8844'
	option authoritative '1'
	option domain 'lan'
	option expandhosts '1'
	list addnhosts '/etc/hosts'
	option boguspriv '0'
	option dnsseccheckunsigned '0'
	option logqueries '1'
	option loglevel '1'
	option rebind_protection '1'
	option domainneeded '1'
	option resolvfile '/etc/resolv.conf.auto'
	list server '/lan/192.168.1.1'
	list server '/dns.google/8.8.4.4'
	list server '/one.one.one.one/1.0.0.1'
	option rebind_localhost '1'
	option readethers '1'
	list interface 'eth0.1'
	list interface 'wlan0'
	list interface 'wlan1'
	list interface 'eth0'
	list interface 'br-lan'
	list interface 'eth0.2'
	list interface 'ifb4eth0.2'
	list interface '6to4-6to4'
	option localservice '0'

config dhcp 'lan'
	option interface 'lan'
	list dns '192.168.1.1'
	list dns '8.8.8.8'
	list dns '8.8.4.4'
	list dns '1.1.1.1'
	list dns '1.0.0.1'
	list dns '2001:4860:4860::8888'
	list dns '2001:4860:4860::8844'
	list server '/lan/192.168.1.1'
	list server '/dns.google/8.8.4.4'
	list server '/one.one.one.one/1.0.0.1'
	option dhcpv6 'server'
	option ra_management '1'
	option leasetime '12h'
	option ra_default '1'
	option ra 'server'
	option force '1'
	option ndp 'hybrid'
	option start '100'
	option limit '150'
	list domain 'lan'

config dhcp 'wan'
	option interface 'wan'
	option ignore '1'
	list dns '192.168.1.1'
	list dns '8.8.8.8'
	list dns '8.8.4.4'
	list dns '1.1.1.1'
	list dns '1.0.0.1'
	list dns '2001:4860:4860::8888'
	list dns '2001:4860:4860::8844'

config odhcpd 'odhcpd'
	option maindhcp '0'
	option leasefile '/tmp/hosts/odhcpd'
	option leasetrigger '/usr/sbin/odhcpd-update'
	option loglevel '1'

